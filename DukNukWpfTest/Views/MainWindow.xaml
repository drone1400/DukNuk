<controls:NukWindow x:Class="DukNuk.Wpf.Test.Views.MainWindow"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:controls="clr-namespace:DukNuk.Wpf.Controls;assembly=DukNukWpf"
                    xmlns:viewModels="clr-namespace:DukNuk.Wpf.Test.ViewModels"
                    mc:Ignorable="d"
                    Title="MainWindow"
                    MinHeight="600" MinWidth="800"
                    Height="600" Width="800"
                    d:DataContext="{d:DesignInstance Type=viewModels:MainWindowViewModel}">
    <!-- put custom status bar content here... -->
    <controls:NukWindow.StatusBarContent>
        <StatusBar>
            <StatusBarItem Content="Status Item #1 " />
            <Separator />
            <StatusBarItem Content="Status Item #2 " />
            <Separator />
            <StatusBarItem Content="Status Item #3 " />
            <Separator />
            <StatusBarItem Content="Status Item #4 (disabled)" IsEnabled="False" />
        </StatusBar>
    </controls:NukWindow.StatusBarContent>

    <!-- put custom menu content here... -->
    <controls:NukWindow.MenuContent>
        <Menu>
            <MenuItem Header="_Menu #1">
                <MenuItem Header="Menu #1.1" />
                <MenuItem Header="Menu #1.2" />
                <MenuItem Header="Menu #1.3" />
                <MenuItem Header="Menu #1.4" />
            </MenuItem>
            <MenuItem Header="M_enu #2">
                <MenuItem Header="Menu #2.1" IsCheckable="True" />
                <MenuItem Header="Menu #2.2" IsCheckable="True" />
                <MenuItem Header="Menu #2.3 (disabled)" IsEnabled="False" />
                <MenuItem Header="Menu #2.4" IsCheckable="True" IsChecked="True" />
            </MenuItem>
            <MenuItem Header="Me_nu #3 (disabled)" IsEnabled="False">
                <MenuItem Header="Menu #3.1" />
                <MenuItem Header="Menu #3.2" />
                <MenuItem Header="Menu #3.3" />
                <MenuItem Header="Menu #3.4" />
            </MenuItem>
            <MenuItem Header="Men_u #4" MaxHeight="300">
                <MenuItem Header="Menu #4.1" />
                <MenuItem Header="Menu #4.2" />
                <MenuItem Header="Menu #4.3" />
                <MenuItem Header="Menu #4.4" />
                <MenuItem Header="Menu #4.5" />
                <MenuItem Header="Menu #4.6" />
                <MenuItem Header="Menu #4.7" />
                <MenuItem Header="Menu #4.8" />
                <MenuItem Header="Menu #4.9" />
                <MenuItem Header="Menu #4.10" />
                <MenuItem Header="Menu #4.11" />
                <MenuItem Header="Menu #4.12" />
                <MenuItem Header="Menu #4.13" />
                <MenuItem Header="Menu #4.14" />
                <MenuItem Header="Menu #4.15" />
                <MenuItem Header="Menu #4.16" />
                <MenuItem Header="Menu #4.17" />
                <MenuItem Header="Menu #4.18" />
                <MenuItem Header="Menu #4.19" />
                <MenuItem Header="Menu #4.20" />
                <MenuItem Header="Menu #4.21" />
                <MenuItem Header="Menu #4.22" />
                <MenuItem Header="Menu #4.23" />
                <MenuItem Header="Menu #4.24" />
                <MenuItem Header="Menu #4.25" />
                <MenuItem Header="Menu #4.26" />
                <MenuItem Header="Menu #4.27" />
                <MenuItem Header="Menu #4.28" />
                <MenuItem Header="Menu #4.29" />
                <MenuItem Header="Menu #4.30" />
                <MenuItem Header="Menu #4.31" />
                <MenuItem Header="Menu #4.32" />
                <MenuItem Header="Menu #4.33" />
                <MenuItem Header="Menu #4.34" />
                <MenuItem Header="Menu #4.35" />
                <MenuItem Header="Menu #4.36" />
                <MenuItem Header="Menu #4.37" />
                <MenuItem Header="Menu #4.38" />
                <MenuItem Header="Menu #4.39" />
                <MenuItem Header="Menu #4.40" />
                <MenuItem Header="Menu #4.41" />
                <MenuItem Header="Menu #4.42" />
                <MenuItem Header="Menu #4.43" />
                <MenuItem Header="Menu #4.44" />
                <MenuItem Header="Menu #4.45" />
                <MenuItem Header="Menu #4.46" />
                <MenuItem Header="Menu #4.47" />
                <MenuItem Header="Menu #4.48" />
                <MenuItem Header="Menu #4.49" />
                <MenuItem Header="Menu #4.50" />
                <MenuItem Header="Menu #4.51" />
                <MenuItem Header="Menu #4.52" />
                <MenuItem Header="Menu #4.53" />
                <MenuItem Header="Menu #4.54" />
                <MenuItem Header="Menu #4.55" />
                <MenuItem Header="Menu #4.56" />
                <MenuItem Header="Menu #4.57" />
                <MenuItem Header="Menu #4.58" />
                <MenuItem Header="Menu #4.59" />
                <MenuItem Header="Menu #4.60" />
                <MenuItem Header="Menu #4.61" />
                <MenuItem Header="Menu #4.62" />
                <MenuItem Header="Menu #4.63" />
                <MenuItem Header="Menu #4.64" />
            </MenuItem>
        </Menu>
    </controls:NukWindow.MenuContent>

    <!-- put main content here... -->
    <Grid>
        <TabControl Name="theBigTabControl">
            <TabItem Header="Controls #1">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Label Grid.Row="0" Grid.Column="0" Content="This is a label"
                               Margin="{Binding MyMargin}" />
                        <Label Grid.Row="0" Grid.Column="1" Content="This is a disabled label" IsEnabled="False"
                               Margin="{Binding MyMargin}" />
                        <TextBlock Grid.Row="1" Grid.Column="0" Text="This is a text block"
                                   Margin="{Binding MyMargin}" />
                        <TextBlock Grid.Row="1" Grid.Column="1" Text="This is a disabled text block" IsEnabled="False"
                                   Margin="{Binding MyMargin}" />
                    </Grid>

                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Button Grid.Row="0" Grid.Column="0" Content="Normal Button"
                                Margin="{Binding MyMargin}">
                            <Button.ToolTip>Hey this button has a tooltip! Nice find!</Button.ToolTip>
                        </Button>
                        <Button Grid.Row="0" Grid.Column="1" Content="Disabled Button" IsEnabled="False"
                                Margin="{Binding MyMargin}" />
                        <ToggleButton Grid.Row="1" Grid.Column="0" Content="Toggle Button"
                                      Margin="{Binding MyMargin}" />
                        <ToggleButton Grid.Row="1" Grid.Column="1" Content="Disabled Toggle Button" IsEnabled="False"
                                      Margin="{Binding MyMargin}" />
                        <RepeatButton Grid.Row="2" Grid.Column="0" Content="Repeat Button"
                                      Margin="{Binding MyMargin}" />
                        <RepeatButton Grid.Row="2" Grid.Column="1" Content="Disabled Repeat Button" IsEnabled="False"
                                      Margin="{Binding MyMargin}" />
                    </Grid>

                    <Grid Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <ComboBox Grid.Row="0" Grid.Column="0" SelectedIndex="0"
                                  Margin="{Binding MyMargin}">
                            <ComboBoxItem>Normal Combo Box</ComboBoxItem>
                            <ComboBoxItem>Another Item 1</ComboBoxItem>
                            <ComboBoxItem>Another Item 2</ComboBoxItem>
                            <ComboBoxItem>Another Item 3</ComboBoxItem>
                            <ComboBoxItem>Another Item 4</ComboBoxItem>
                            <ComboBoxItem>Another Item 5</ComboBoxItem>
                        </ComboBox>
                        <ComboBox Grid.Row="0" Grid.Column="1" IsEnabled="False" SelectedIndex="0"
                                  Margin="{Binding MyMargin}">
                            <ComboBoxItem>Disabled Combo Box</ComboBoxItem>
                            <ComboBoxItem>Another Item 1</ComboBoxItem>
                            <ComboBoxItem>Another Item 2</ComboBoxItem>
                            <ComboBoxItem>Another Item 3</ComboBoxItem>
                            <ComboBoxItem>Another Item 4</ComboBoxItem>
                            <ComboBoxItem>Another Item 5</ComboBoxItem>
                        </ComboBox>
                        <ComboBox Grid.Row="1" Grid.Column="0" IsEditable="True" SelectedIndex="0"
                                  Margin="{Binding MyMargin}">
                            <ComboBoxItem>Editable Combo Box</ComboBoxItem>
                            <ComboBoxItem>Another Item 1</ComboBoxItem>
                            <ComboBoxItem>Another Item 2</ComboBoxItem>
                            <ComboBoxItem>Another Item 3</ComboBoxItem>
                            <ComboBoxItem>Another Item 4</ComboBoxItem>
                            <ComboBoxItem>Another Item 5</ComboBoxItem>
                        </ComboBox>
                        <ComboBox Grid.Row="1" Grid.Column="1" IsEditable="True" IsEnabled="False" SelectedIndex="0"
                                  Margin="{Binding MyMargin}">
                            <ComboBoxItem>Disabled Editable Combo Box</ComboBoxItem>
                            <ComboBoxItem>Another Item 1</ComboBoxItem>
                            <ComboBoxItem>Another Item 2</ComboBoxItem>
                            <ComboBoxItem>Another Item 3</ComboBoxItem>
                            <ComboBoxItem>Another Item 4</ComboBoxItem>
                            <ComboBoxItem>Another Item 5</ComboBoxItem>
                        </ComboBox>
                    </Grid>

                    <Grid Grid.Row="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBox Grid.Row="0" Grid.Column="0" Text="Normal Text Box"
                                 Margin="{Binding MyMargin}" />
                        <TextBox Grid.Row="0" Grid.Column="1" Text="Disabled Text Box" IsEnabled="False"
                                 Margin="{Binding MyMargin}" />
                        <PasswordBox Grid.Row="1" Grid.Column="0" Password="Secret Password! Don't peek!"
                                     Margin="{Binding MyMargin}" />
                        <PasswordBox Grid.Row="1" Grid.Column="1" Password="Disabled Secret Password! Don't peek!"
                                     IsEnabled="False"
                                     Margin="{Binding MyMargin}" />

                        <RichTextBox Grid.Row="2" Grid.Column="0"
                                     Margin="{Binding MyMargin}">
                            <FlowDocument>
                                <Paragraph xml:space="preserve">This is a Rich Text Box!</Paragraph>
                            </FlowDocument>
                        </RichTextBox>
                        <RichTextBox Grid.Row="2" Grid.Column="1" IsEnabled="False"
                                     Margin="{Binding MyMargin}">
                            <FlowDocument>
                                <Paragraph xml:space="preserve">This is a disabled Rich Text Box!</Paragraph>
                            </FlowDocument>
                        </RichTextBox>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Controls #2">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <CheckBox Grid.Row="0" Grid.Column="0" Content="This is a CheckBox"
                                  Margin="{Binding MyMargin}" />
                        <CheckBox Grid.Row="0" Grid.Column="1" Content="This is a disabled CheckBox"
                                  Margin="{Binding MyMargin}"
                                  IsEnabled="False" />
                        <CheckBox Grid.Row="1" Grid.Column="0" Content="This is a CheckBox"
                                  Margin="{Binding MyMargin}"
                                  IsChecked="True" />
                        <CheckBox Grid.Row="1" Grid.Column="1" Content="This is a disabled CheckBox"
                                  Margin="{Binding MyMargin}"
                                  IsChecked="True"
                                  IsEnabled="False" />

                        <CheckBox Grid.Row="2" Grid.Column="0" Content="This is a CheckBox"
                                  Margin="{Binding MyMargin}"
                                  controls:CheckBoxHelper.RightSideCheckMark="True" />
                        <CheckBox Grid.Row="2" Grid.Column="1" Content="This is a disabled CheckBox"
                                  Margin="{Binding MyMargin}"
                                  IsEnabled="False"
                                  controls:CheckBoxHelper.RightSideCheckMark="True" />
                        <CheckBox Grid.Row="3" Grid.Column="0" Content="This is a CheckBox"
                                  Margin="{Binding MyMargin}"
                                  IsChecked="True"
                                  controls:CheckBoxHelper.RightSideCheckMark="True" />
                        <CheckBox Grid.Row="3" Grid.Column="1" Content="This is a disabled CheckBox"
                                  Margin="{Binding MyMargin}"
                                  IsChecked="True"
                                  IsEnabled="False"
                                  controls:CheckBoxHelper.RightSideCheckMark="True" />

                    </Grid>
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <RadioButton Grid.Row="0" Grid.Column="0" Content="This is a Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio1" />
                        <RadioButton Grid.Row="0" Grid.Column="1" Content="This is a disabled Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio2"
                                     IsEnabled="False" />
                        <RadioButton Grid.Row="1" Grid.Column="0" Content="This is a Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio1" />
                        <RadioButton Grid.Row="1" Grid.Column="1" Content="This is a disabled Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio2"
                                     IsEnabled="False" />
                        <RadioButton Grid.Row="2" Grid.Column="0" Content="This is a Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio1"
                                     IsChecked="True"
                                     controls:CheckBoxHelper.RightSideCheckMark="True" />
                        <RadioButton Grid.Row="2" Grid.Column="1" Content="This is a disabled Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio2"
                                     IsChecked="True"
                                     IsEnabled="False"
                                     controls:CheckBoxHelper.RightSideCheckMark="True" />
                        <RadioButton Grid.Row="3" Grid.Column="0" Content="This is a Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio1"
                                     controls:CheckBoxHelper.RightSideCheckMark="True" />
                        <RadioButton Grid.Row="3" Grid.Column="1" Content="This is a disabled Radio Button"
                                     Margin="{Binding MyMargin}"
                                     GroupName="Radio2"
                                     IsEnabled="False"
                                     controls:CheckBoxHelper.RightSideCheckMark="True" />
                    </Grid>

                    <Grid Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Expander
                            Grid.Column="0" Grid.Row="0"
                            Header="Expander"
                            IsExpanded="True">
                            <RichTextBox Margin="{Binding MyMargin}">
                                <FlowDocument>
                                    <Paragraph xml:space="preserve">This is a Rich Text Box!</Paragraph>
                                </FlowDocument>
                            </RichTextBox>
                        </Expander>

                        <Expander
                            Grid.Column="1" Grid.Row="0"
                            Header="Disabled Expander"
                            IsEnabled="False"
                            IsExpanded="True">
                            <RichTextBox Margin="{Binding MyMargin}">
                                <FlowDocument>
                                    <Paragraph xml:space="preserve">This is a Rich Text Box!</Paragraph>
                                </FlowDocument>
                            </RichTextBox>
                        </Expander>
                    </Grid>


                    <Grid Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Label Grid.Column="0" Content="Select Theme Color: " />
                        <ComboBox Grid.Column="1" ItemsSource="{Binding AvailableThemes}"
                                  SelectedValue="{Binding SelectedTheme}" />
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Controls #3">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Slider Grid.Row="0" Grid.Column="0"
                                Margin="{Binding MyMargin}"
                                TickPlacement="TopLeft" />
                        <Slider Grid.Row="1" Grid.Column="0"
                                Margin="{Binding MyMargin}"
                                TickPlacement="BottomRight" />
                        <Slider Grid.Row="2" Grid.Column="0"
                                Margin="{Binding MyMargin}"
                                TickPlacement="Both" />
                        <Slider Grid.Row="3" Grid.Column="0"
                                x:Name="SliderTest3A"
                                Margin="{Binding MyMargin}"
                                TickPlacement="None" />

                        <Slider Grid.Row="0" Grid.Column="1"
                                Margin="{Binding MyMargin}"
                                TickPlacement="TopLeft"
                                IsEnabled="False" />
                        <Slider Grid.Row="1" Grid.Column="1"
                                Margin="{Binding MyMargin}"
                                TickPlacement="BottomRight"
                                IsEnabled="False" />
                        <Slider Grid.Row="2" Grid.Column="1"
                                Margin="{Binding MyMargin}"
                                TickPlacement="Both"
                                IsEnabled="False" />
                        <Slider Grid.Row="3" Grid.Column="1"
                                x:Name="SliderTest3D"
                                Margin="{Binding MyMargin}"
                                TickPlacement="None"
                                IsEnabled="False" />

                        <ProgressBar Grid.Row="4" Grid.Column="0"
                                     Margin="{Binding MyMargin}"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}" />

                        <ProgressBar Grid.Row="4" Grid.Column="1"
                                     Margin="{Binding MyMargin}"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}"
                                     IsEnabled="False" />

                        <ProgressBar Grid.Row="5" Grid.Column="0"
                                     Margin="{Binding MyMargin}"
                                     IsIndeterminate="True"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}" />

                        <ProgressBar Grid.Row="5" Grid.Column="1"
                                     Margin="{Binding MyMargin}"
                                     IsIndeterminate="True"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}"
                                     IsEnabled="False" />
                    </Grid>

                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Slider Grid.Row="0" Grid.Column="0"
                                Margin="{Binding MyMargin}"
                                TickPlacement="TopLeft"
                                Orientation="Vertical" />
                        <Slider Grid.Row="0" Grid.Column="1"
                                Margin="{Binding MyMargin}"
                                TickPlacement="BottomRight"
                                Orientation="Vertical" />
                        <Slider Grid.Row="0" Grid.Column="2"
                                Margin="{Binding MyMargin}"
                                TickPlacement="Both"
                                Orientation="Vertical" />
                        <Slider Grid.Row="0" Grid.Column="3"
                                Margin="{Binding MyMargin}"
                                TickPlacement="None"
                                Orientation="Vertical" />

                        <Slider Grid.Row="0" Grid.Column="6"
                                Margin="{Binding MyMargin}"
                                TickPlacement="TopLeft"
                                IsEnabled="False"
                                Orientation="Vertical" />
                        <Slider Grid.Row="0" Grid.Column="7"
                                Margin="{Binding MyMargin}"
                                TickPlacement="BottomRight"
                                IsEnabled="False"
                                Orientation="Vertical" />
                        <Slider Grid.Row="0" Grid.Column="8"
                                Margin="{Binding MyMargin}"
                                TickPlacement="Both"
                                IsEnabled="False"
                                Orientation="Vertical" />
                        <Slider Grid.Row="0" Grid.Column="9"
                                Margin="{Binding MyMargin}"
                                TickPlacement="None"
                                IsEnabled="False"
                                Orientation="Vertical" />

                        <ProgressBar Grid.Row="0" Grid.Column="4"
                                     Margin="{Binding MyMargin}"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}"
                                     Orientation="Vertical" />

                        <ProgressBar Grid.Row="0" Grid.Column="5"
                                     Margin="{Binding MyMargin}"
                                     IsIndeterminate="True"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}"
                                     Orientation="Vertical" />

                        <ProgressBar Grid.Row="0" Grid.Column="10"
                                     Margin="{Binding MyMargin}"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}"
                                     IsEnabled="False"
                                     Orientation="Vertical" />

                        <ProgressBar Grid.Row="0" Grid.Column="11"
                                     Margin="{Binding MyMargin}"
                                     IsIndeterminate="True"
                                     Minimum="{Binding Minimum, ElementName=SliderTest3A}"
                                     Maximum="{Binding Maximum, ElementName=SliderTest3A}"
                                     Value="{Binding Value, ElementName=SliderTest3A}"
                                     IsEnabled="False"
                                     Orientation="Vertical" />
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Controls Custom">
                <controls:NukScrollViewer>
                    <Grid>
                        <Grid.Resources>
                            <Style TargetType="Label" BasedOn="{StaticResource {x:Type Label}}">
                                <Setter Property="HorizontalAlignment" Value="Center" />
                            </Style>
                        </Grid.Resources>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <Grid Grid.Row="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Label Grid.Row="0" Grid.Column="0" Content="NukCounterDouble" />
                            <controls:NukCounterDouble
                                Grid.Row="0" Grid.Column="1"
                                Value="123.45" />
                            <Label Grid.Row="1" Grid.Column="0" Content="NukCounterDouble (disabled)" />
                            <controls:NukCounterDouble
                                Grid.Row="1" Grid.Column="1"
                                Value="123.45"
                                IsEnabled="False" />
                        </Grid>


                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Label Grid.Row="0" Grid.Column="0" Content="NukCounterInt" />
                            <controls:NukCounterInt
                                Grid.Row="0" Grid.Column="1"
                                Value="100" />

                            <Label Grid.Row="1" Grid.Column="0" Content="NukCounterInt (disabled)" />
                            <controls:NukCounterInt
                                Grid.Row="1" Grid.Column="1"
                                Value="100"
                                IsEnabled="False" />

                            <Label Grid.Row="2" Grid.Column="0" Content="NukCounterInt HEX" />
                            <controls:NukCounterInt
                                Grid.Row="2" Grid.Column="1"
                                IsHexaDecimalMode="True"
                                Value="100" />
                        </Grid>

                        <Grid Grid.Row="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
